{
  "manifest_version": 2,
  "name": "Simple Browser Extention",
  "description": "using React, Chrome, and Redux",
  "version": "0.0.0.1",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "RCR Example",
    "default_popup": "popup.html",
    "default_icon": "favicon.ico"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": [],
      "js": []
    }
  ]
}
